   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"2", ",", "1"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"2", ",", "2"}], "}"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"LittlewoodRichardsonCoefficients", "[", "R", "]"}]}], "Input",
 CellLabel->"In[1]:=",
 CellID->250692952,ExpressionUUID->"52da26f6-672a-4f46-82fc-63e00400a0d6"],

